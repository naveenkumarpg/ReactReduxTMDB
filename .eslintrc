{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "sourceType": "module"
  },
  "globals": {
    "$": true,
    "d3": true,
    "cyear": true,
    "data": true,
    "primatyType": true,
    "jQuery": true,
    "window": true,
    "Promise": true
  },
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "eqeqeq": "error",
    "no-eq-null": "error",
    "no-eval": "error",
    "array-bracket-spacing": "error",
    "comma-spacing": "error",
    "indent": [
      "error",
      2
    ],
    "space-before-function-paren": 0,
    "spaced-comment": "error",
    "newline-before-return": "error",
    "key-spacing": "error",
    "keyword-spacing": "error",
    "no-undef": "warn",
    "no-unused-vars": [
      1,
      {
        "vars": "local",
        "args": "none",
        "varsIgnorePattern": "^_"
      }
    ]
  }
}